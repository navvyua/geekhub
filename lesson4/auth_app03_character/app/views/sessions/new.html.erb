<h1>Sign In</h1>

<%= form_tag create_path do %>
  <p>
    <%= label_tag :username %><br />
    <%= text_field_tag :username %>
  </p>

  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </p>

  <p><%= submit_tag 'Sign In' %></p>
<% end %>

<%= link_to 'Sign Up', sign_up_path %>