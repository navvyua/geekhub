<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Delete', admin_user_path(user), method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>